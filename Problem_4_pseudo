for x = 10..99
    for y = 10..99
    	product = x*y (100-9801)
	if product/1000 = 0 (three digit num)	
	   hundreds = product/100
	   newnum1 = product%100
	   tens = newnum1/10
	   newnum2 = newnum1%10
	   ones = newnum2

	   if hundreds == ones 
	      highest_palind = product
	   end
	else
	   thousands = product/1000
	   newnum1 = product%1000
	   hundreds = newnum1/100
	   newnum2 = newnum1%100
	   tens = newnum2/10	 
	   newnum3 = newnum2%10
	   ones = newnum3

	   	if (thousands == ones) && (hundreds == tens)
		highest_palind = product
		end
	end
  end
end
puts highest_palind   	   

		



